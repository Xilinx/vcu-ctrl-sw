INCLUDES+=-I$(SOFT)/include

CONFIG?=$(SOFT)/include/config.h
ifneq ($(CONFIG),)
CFLAGS+=-include $(CONFIG)
endif

include base.mk
include $(PLUGIN)/plugin.mk

true_all: $(TARGETS)
