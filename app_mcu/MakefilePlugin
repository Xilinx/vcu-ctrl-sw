PLUGIN_SCRIPT_DIR=$(PLUGIN)/scripts
LINK_SCRIPT=-T plugin/plugin.ld

LIBS:=\
  lib/libm.a\
  lib/libc.a\
  lib/libgloss.a\
  lib/libxil.a

all: true_all

AL5E?=../../Firmware/al5e.fw
AL5E_B?=../../Firmware/al5e_b.fw
SOFT?=..
INCLUDES+=-I$(SOFT)/extra/include
